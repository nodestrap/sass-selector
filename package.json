{
  "name": "@nodestrap/sass-selector",
  "version": "2.0.3",
  "description": "A sass utility for manipulating selectors.",
  "dependencies": {
    "@nodestrap/sass-string": "^2.0.7"
  },
  "devDependencies": {
    "tilde-sass": "^1.0.2"
  },
  "scripts": {
    "test": "tilde-sass --file test/*.scss --outFile test/ --mergeSelectors",
    "test-watch": "tilde-sass --file test/*.scss --outFile test/ --mergeSelectors --watch",
    "deploy": "tilde-sass --file src/*.scss --outFile dist/ --mergeSelectors --outputStyle compressed",
    "deploy-watch": "tilde-sass --file src/*.scss --outFile dist/ --mergeSelectors --outputStyle compressed --watch"
  },
  "cssnano": {
    "preset": [
      "default",
      {
        "calc": false
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodestrap/sass-selector.git"
  },
  "keywords": [
    "sass",
    "utility",
    "selector"
  ],
  "author": "nodestrap, heymarco",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nodestrap/sass-selector/issues"
  },
  "homepage": "https://github.com/nodestrap/sass-selector#readme"
}
